    // adds the health powerup randomly on the map 
    public void HealthPowerUp(){
        if (Greenfoot.getRandomNumber(1000) < 300)
        {
            HealthPowerUp h = new HealthPowerUp();
            addObject (h, Greenfoot.getRandomNumber(917) + 84,  Greenfoot.getRandomNumber(531) + 103);
        }
    }

    public void DamageUp(){
        if (Greenfoot.getRandomNumber(1000) < 300)
        {
            DamageUp d = new DamageUp();
            addObject (d, Greenfoot.getRandomNumber(917) + 84,  Greenfoot.getRandomNumber(531) + 103);
        }
    }
}
